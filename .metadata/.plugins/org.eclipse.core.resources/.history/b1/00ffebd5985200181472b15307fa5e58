public class TimeClass3 {

	private int month;
	private int day;
	private int year;

	public TimeClass3(int month, int day, int year) {
		this.month = month;
		this.day = day;
		this.year = year;

		System.out.printf("Constructor is for %s\n", this);
		// "this" in print format will Override method of to String
	}

	@Override
	public String toString() {
		return "TimeClass{" + "month=" + month + ", day=" + day + ", year="
				+ year;
	}
}
