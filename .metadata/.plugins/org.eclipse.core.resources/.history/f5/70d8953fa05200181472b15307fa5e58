// Enumeration to make Constants

public enum EnumExp {

	// Constants (with two String factors)
	Ehsan("Good boy", "26"), Mohamad("Best friend", "26");

	private final String desc;
	private final String year;

	// final means that name we choose here never able to change

	// Constructor
	private EnumExp(String desc, String year) {
		this.desc = desc;
		this.year = year;
	}

}
