//JFrame lesson with Code

import java.awt.FlowLayout; //for button

import javax.swing.JFrame; //for frame
import javax.swing.JLabel;

public class JFrameTest2 extends JFrame {

	private JLabel item1;

	public JFrameTest2() {
		super("My Windows"); /*
							 * FIRST is for "title", we super it to parent class
							 */
		setLayout(new FlowLayout()); // flow buttons layout
		item1 = new JLabel("Salam");
	}
}
