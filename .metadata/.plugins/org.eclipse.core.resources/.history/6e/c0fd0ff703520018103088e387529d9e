public class TimeClass2 {

	private int hour;
	private int minute;
	private int second;

	public TimeClass2() {
		// Constructor
		this(0, 0, 0);
	}

	public TimeClass2(int h) {
		// Overload Constructor
		this(h, 0, 0);

	}

	public TimeClass2(int h, int m) {
		// Overload Constructor
		this(h, m, 0);

	}

	public TimeClass2(int h, int m, int s) {
		// Overload Constructor
		this(h, m, s);
	}
}
